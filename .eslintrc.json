{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaFeatures": {"experimentalObjectRestSpread": true},
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "semi": [
            "error",
            "never"
        ],
        "react/prop-types": 0,
        "import/extensions": 0,
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
    }
}
